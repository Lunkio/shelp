              "license_type": {
                "id": 1,
                "name": "All rights reserved."
              }
            },
            {
              "url": "https://edit.myhelsinki.fi/sites/default/files/styles/api_1980x1020/public/2017-07/Helsinki_HakaniemiMarketHall_Elina%20Sirparanta%20VF.jpg?h=6f8e8448&itok=TBF0UmR3",
              "copyright_holder": "Elina Sirparanta",
